<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp3.0</TargetFramework>
        <PackageVersion>3.0.5</PackageVersion>
        <Authors>Akaion</Authors>
        <Description>A Windows native DLL injection library that supports several methods of injection.</Description>
        <RepositoryUrl>https://github.com/Akaion/Bleak</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DocumentationFile>bin\Release\Bleak.xml</DocumentationFile>
    </PropertyGroup>

</Project>
